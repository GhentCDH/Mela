{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend/src",
  "projectType": "application",
  "tags": [
    "type:api",
    "scope:backend"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "generated-types:build",
        "generated-prisma:build",
        "generated-dto:build"
      ],
      "outputs": ["{workspaceRoot}/dist/apps/backend"],
      "options": {
        "commands": [
          "cd apps/backend && vite build"
        ]
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "apps/backend"
      }
    }
  },
  "implicitDependencies": ["generated-types", "generated-prisma", "generated-dto"]
}
