{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/frontend/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "generated-types:build"
      ],
      "outputs": [
        "{workspaceRoot}/dist/apps/frontend"
      ],
      "options": {
        "commands": [
          "cd apps/frontend && vite build"
        ]
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "apps/frontend"
      }
    }
  },
  "implicitDependencies": [
    "generated-types",
    "generated-dto"
  ]
}
